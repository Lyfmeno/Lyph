#include "CLASSNAME.hpp"

/////////////////////////////
// Coplien                 //
/////////////////////////////
CLASSNAME::CLASSNAME()
{
	if (CLASSNAME_LOG == 1)
		std::cout << "CLASSNAME default constructor called" << std::endl;
}

CLASSNAME::CLASSNAME(const CLASSNAME &cpy)
{
	if (CLASSNAME_LOG == 1)
		std::cout << "CLASSNAME copy constructor called" << std::endl;
	// ! Make copy ! this->thing = cpy->thing
}

CLASSNAME::~CLASSNAME()
{
	if (CLASSNAME_LOG == 1)
		std::cout << "CLASSNAME default destructor called" << std::endl;
}

CLASSNAME &CLASSNAME::operator=(const CLASSNAME &rhs)
{
	if (this != &rhs)
	{
		// ! Make copy !
	}
	return (*this);
}

